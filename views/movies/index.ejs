<%- include('../layout/header')%>
<%- include('../layout/navbar')%>
    <table id="list">
        <thead>
            <tr>
                <th> title </th>
                <th> releaseYear </th>
                <th>mpaa</th>
            </tr>
        </thead>
      <tbody>
        <% movies.forEach(function(m){ %>
        <tr>
          <td><%= m.title%></td>
          <td><%= m.releaseYear%></td>
          <td><a href="/movies/<%= m._id %>">DETAILS</a>  </td>

        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
